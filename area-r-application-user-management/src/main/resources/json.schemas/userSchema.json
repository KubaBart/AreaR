{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "User",
  "description": "user",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for a user",
      "type": "integer"
    },
    "userName": {
      "description": "Name of user",
      "type": "string"
    },
    "password": {
      "type": "number",
      "minimum": 0,
      "exclusiveMinimum": true
    }
  }
}